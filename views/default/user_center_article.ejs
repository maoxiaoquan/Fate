<!--article-list-lay layout-content start-->
<section class="user-lay layout-content">
    <div class="container  box-container">
        <div class="row">

            <div class="col-xs-12 col-sm-8 col-md-8 main">

                <% include user_center/user_center_top.ejs %>

                <ul class="topic-list">
                    <li class="title">个人专题：</li>
                    <li class="current"><a href="">全部</a></li>
                    <li><a href="">HTML</a></li>
                    <li><a href="">CSS3</a></li>
                    <li><a href="">HTML</a></li>
                    <li><a href="">CSS3</a></li>
                    <li><a href="">HTML</a></li>
                    <li><a href="">CSS3</a></li>
                    <li><a href="">HTML</a></li>
                    <li><a href="">CSS3</a></li>
                    <li><a href="">HTML</a></li>
                    <li><a href="">CSS3</a></li>
                    <li><a href="">HTML</a></li>
                    <li><a href="">CSS3</a></li>
                </ul>

                <div id="list-container">
                    <!-- 文章列表模块 -->
                    <ul class="note-list">
                        <% data.article_list.forEach(function (item, key) { %>
                        <li class="">
                            <article class="content-box article-list">
                                <div class="info-box">
                                    <div class="info-row title-row">
                                        <a href="/article" class="title"><%= item.title %></a>
                                    </div>
                                    <div class="info-row meta-row">
                                        <ul class="meta-list">
                                            <li class="item category">
                                                <span class="category-title frontend">前端</span>
                                            </li>
                                            <li class="item username clickable">
                                                <div class="user-popover-box">
                                                    <a href="">花裤衩</a>
                                                </div>
                                            </li>
                                            <li class="item">10月前</li>
                                            <li class="item">25781 次阅读</li>
                                            <li><a href="">编辑</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </article>
                        </li>
                        <% }) %>
                    </ul>
                </div>


            </div>

            <div class="col-xs-12 col-sm-4 col-md-4 aside">
                <% include user_center/user_center_aside.ejs %>
            </div>

        </div>
    </div>
</section>
<!--article-list-lay layout-content end-->


