                            HTML5
                        </a>
                    </div>
                    <div class="col-grid col-xs-12 col-sm-2 col-md-2">
                        <a class="layout-type-item"
                           style="background-image: url('/default/img/2017100416064445-e1512811758795.jpeg');"
                           href="">
                            HTML5
                        </a>
                    </div>
                    <div class="col-grid col-xs-12 col-sm-2 col-md-2">
                        <a class="layout-type-item"
                           style="background-image: url('/default/img/2017100416064445-e1512811758795.jpeg');"
                           href="">
                            HTML5
                        </a>
                    </div>
                    <div class="col-grid col-xs-12 col-sm-3 col-md-3">
                        <a class="layout-type-item"
                           style="background-image: url('/default/img/2017100416064445-e1512811758795.jpeg');"
                           href="">
                            HTML5
                        </a>
                    </div>
                    <div class="col-grid col-xs-12 col-sm-3 col-md-3">
                        <a class="layout-type-item"
                           style="background-image: url('/default/img/2017100416064445-e1512811758795.jpeg');"
                           href="">
                            HTML5
                        </a>
                    </div>
                    <div class="col-grid col-xs-12 col-sm-2 col-md-2">
                        <a class="layout-type-item"
                           style="background-image: url('/default/img/2017100416064445-e1512811758795.jpeg');"
                           href="">
                            HTML5
                        </a>
                    </div>
                    <div class="col-grid col-xs-12 col-sm-3 col-md-3">
                        <a class="layout-type-item"
                           style="background-image: url('/default/img/2017100416064445-e1512811758795.jpeg');"
                           href="">
                            HTML5
                        </a>
                    </div>
                    <div class="col-grid col-xs-12 col-sm-4 col-md-4">
                        <a class="layout-type-more" href="">
                            更多热门专题 <i class="iconfont icon-more"></i>
                        </a>
                    </div>
                </div>

            </section>
            <!--home-lay layout-type end-->

            <!--home-lay layout-content start-->
            <section class="home-lay layout-content">

               <!-- <div class="filter-menu text-center">
                    <button class="active">文章</button>
                    <button data-cid="945">说说</button>
                    <button data-cid="945">视频</button>
                    <button data-cid="945">公告</button>
                </div>-->

                <ul class="article-list">
                    <% data.article_list.forEach(function (item, key) { %>
                    <li>
                        <article class="content-box">
                            <div class="info-box">
                                <div class="info-row title-row">
                                    <a href="/article/<%= item.dataValues.aid %>" class="title"><%= item.dataValues.title %></a>
                                </div>
                                <div class="info-row meta-row">
                                    <ul class="meta-list">
                                        <li class="item category">
                                            <span class="category-title frontend"><%= item.dataValues.category %></span>
                                        </li>
                                        <li class="item username clickable">
                                            <div class="user-popover-box">
                                                <a href=""><%= item.dataValues.author %></a>
                                            </div>
                                        </li>
                                        <li class="item">10月前</li>
                                        <li class="item"><%= item.dataValues.read_count %> 次阅读</li>
                                    </ul>
                                </div>
                            </div>
                            <div class="lazy thumb thumb loaded"
                                 style="background-image: url('/default/img/2017100416064445-e1512811758795.jpeg'); background-size: cover;">
                            </div>
                        </article>
                    </li>
                    <% }) %>
                </ul>


                <div class="nav-links">
                    <span aria-current="page" class="page-numbers current">1</span>
                    <a class="page-numbers" href="http://cosy.codenote.io/page/2">2</a>
                    <span class="page-numbers dots">…</span>
                    <a class="page-numbers" href="http://cosy.codenote.io/page/15">15</a>
                    <a class="next page-numbers" href="http://cosy.codenote.io/page/2">
                        <i class="fa fa fa-angle-right"></i>
                    </a>
                </div>
            </section>
