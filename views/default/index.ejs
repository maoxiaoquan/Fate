<!--header.ejs start-->
<% include parts/header.ejs %>
<!--header.ejs end-->

<body class="black-theme">

<!--header_view.ejs start-->
<% include parts/header_view.ejs %>
<!--header_view.ejs end-->


<div class="container box-container">

    <div class="side-menu">
        <!--home-lay layout-type start-->

        <section class="side-menu-view ">

            <div class="recommend-collection">

                <div class="board">
                    <a class="board-content" href="">
                        热门
                    </a>
                </div>

                <% data.article_column.forEach(function (item, key) { %>
                <a class="collection clearfix" href="">
                    <i class="iconfont collection-img <%= item.article_column_icon %>"></i>
                    <span class="collection-name"><%= item.article_column_name %></span>
                </a>
                <% }) %>

                <a class="collection" href="/subscribe/tag">
                    <i class="iconfont icon-more1 collection-img "></i>
                    <span class="collection-name">
                    更多
                    </span>
                </a>
            </div>
        </section>
        <!--home-lay layout-type end-->
    </div>

    <div class="row" style="margin-left: 125px">
        <div class="col-xs-12 col-sm-8 col-md-8">


            <div id="box-swiper" class="carousel slide box-swiper" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#box-swiper" data-slide-to="0" class="active"></li>
                    <li data-target="#box-swiper" data-slide-to="1"></li>
                    <li data-target="#box-swiper" data-slide-to="2"></li>
                </ol>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="/default/img/66677.png" alt="First slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="/default/img/66677.png" alt="Second slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="/default/img/66677.png" alt="Third slide">
                    </div>
                </div>
                <a class="carousel-control-prev" href="#box-swiper" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#box-swiper" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>


            <!--home-lay layout-content start-->
            <section class="home-lay layout-content">

                <!-- <div class="filter-menu text-center">
                     <button class="active">文章</button>
                     <button data-cid="945">说说</button>
                     <button data-cid="945">视频</button>
                     <button data-cid="945">公告</button>
                 </div>-->

                <ul class="article-list">
                    <% data.article_list.forEach(function (item, key) { %>
                    <li>
                        <article class="content-box content-box-index">
                            <div class="info-box">
                                <div class="info-row title-row">
                                    <a href="/article/<%= item.aid %>"
                                       class="title"><%= item.title %></a>
                                </div>
                                <!-- <div class="content-text">
                                    <%= item.content %>
                                </div>-->
                                <div class="info-row meta-row">
                                    <ul class="meta-list">
                                        <li class="item category">
                                            <span class="category-title frontend"><%= item.category %></span>
                                        </li>
                                        <li class="item username clickable">
                                            <div class="user-popover-box">
                                                <a href=""><%= item.author %></a>
                                            </div>
                                        </li>
                                        <li class="item">10月前</li>
                                        <li class="item"><%= item.read_count %> 次阅读</li>
                                    </ul>
                                </div>
                            </div>
                            <div class="lazy thumb thumb loaded"
                                 style="background-image: url('/default/img/2017100416064445-e1512811758795.jpeg'); background-size: cover;">
                            </div>
                        </article>
                    </li>
                    <% }) %>
                </ul>


                <div class="nav-links">
                    <span aria-current="page" class="page-numbers current">1</span>
                    <a class="page-numbers" href="http://cosy.codenote.io/page/2">2</a>
                    <span class="page-numbers dots">…</span>
                    <a class="page-numbers" href="http://cosy.codenote.io/page/15">15</a>
                    <a class="next page-numbers" href="http://cosy.codenote.io/page/2">2
                    </a>
                </div>
            </section>
            <!--home-lay layout-content end-->


        </div>
        <div class="col-xs-12 col-sm-4 col-md-4">
            <!--aside.ejs start-->
            <% include parts/aside.ejs %>
            <!--aside.ejs end-->
        </div>
    </div>
</div>


<!--footer_view.ejs start-->
<% include parts/footer_view.ejs %>
<!--footer_view.ejs end-->


